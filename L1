ASSUME CS:CODE,DS:DATA

DATA SEGMENT
    OP1 DB 20H
    
DATA ENDS

CODE SEGMENT
    START:
    MOV AX,DATA
    MOV DS,AX
    MOV AL,OP1
    AND AL, 0F0H
    MOV BL,04H
    AGAIN: ROR AL,01
    DEC BL
    TEST BL,0FFH
    JNZ AGAIN
    MOV BL,06H
    MUL BL
    MOV AH,OP1
    SUB AH,AL
    MOV DI, 0000H
    MOV [DI],AH
    MOV AH,4CH
    INT 21H
    CODE ENDS
END START

